{
  "name": "express-n8n-backend",
  "version": "1.0.0",
  "description": "Backend API untuk meneruskan chat request ke n8n webhook",
  "main": "dist/server/bootstrap.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server/bootstrap.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/server/bootstrap.js",
    "vercel-build": "echo 'Building for Vercel...' && npm run build",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "prisma": "^6.19.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
